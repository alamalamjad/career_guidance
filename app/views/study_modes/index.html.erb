<p id="notice"><%= notice %></p>

<h1>Listing Study Modes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @study_modes.each do |study_mode| %>
      <tr>
        <td><%= study_mode.name %></td>
        <td><%= link_to 'Show', study_mode %></td>
        <td><%= link_to 'Edit', edit_study_mode_path(study_mode) %></td>
        <td><%= link_to 'Destroy', study_mode, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Study mode', new_study_mode_path %>
